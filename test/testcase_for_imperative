import unittest
from orginal import floydWarshall, INF

class TestFloydWarshallRecursive(unittest.TestCase):
    def test_floydWarshall(self):
        # Test case 1
        graph1 = [[0, 5, INF, 10],
                 [INF, 0, 3, INF],
                 [INF, INF, 0,   1],
                 [INF, INF, INF, 0]
                 ]
        expected_output1 = [[0, 5, 8, 9],
                          [INF, 0, 3, 4],
                          [INF, INF, 0, 1],
                          [INF, INF, INF, 0]]
        self.assertEqual(floydWarshall(graph1), expected_output1)
        graph2 = [[0, INF, -2, INF],
                 [4, 0, 3, INF],
                 [INF, INF, 0, 2],
                 [INF, -1, INF, 0]
                 ]
        expected_output2 = [[0, -1, -2, 0],
                          [4, 0, 2, 4],
                          [5, 1, 0, 2],
                          [3, -1, 1, 0]]
if __name__ == '__main__':
    unittest.main()
